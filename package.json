{
  "name": "taikonotes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "autoprefixer": "^10.3.4",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "tailwindcss": "^2.2.15"
  },
  "scripts": {
    "start": "npm run build:style && react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:style": "tailwind build -i src/index.css -c tailwind.config.js -o src/tailwind.css",
    "build": "npm run build:style && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fixSourceMaps": "sed -i '' 's/sourceMap: isEnvProduction && /sourceMap: /g' node_modules/react-scripts/config/webpack.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.0"
  }
}
