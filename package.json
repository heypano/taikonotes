{
  "name": "taikonotes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "autoprefixer": "^10.3.4",
    "postcss-cli": "^8.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-scripts": "^4.0.3",
    "tailwindcss": "^2.2.15"
  },
  "scripts": {
    "start:style": "tailwindcss -i src/index.css -c tailwind.config.js -o src/tailwind.css --watch",
    "start:react": "react-scripts start",
    "start": "npm-run-all --parallel start:style start:react",
    "dev": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:style": "tailwind build -i src/index.css -c tailwind.config.js -o src/tailwind.css",
    "build": "npm run build:style && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fixSourceMaps": "sed -i '' 's/sourceMap: isEnvProduction && /sourceMap: /g' node_modules/react-scripts/config/webpack.config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.0",
    "npm-run-all": "^4.1.5"
  }
}
